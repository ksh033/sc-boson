{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "build": "pnpm --filter=\"@scboson/**\" build",
    "build:dev": "pnpm  --filter=@scboson/sc-layout build:dev",

    "changelog": "pro-changelog",
    "check-deps": "node ./scripts/checkDeps.js",
    "createRelease": "node ./scripts/createRelease.js",
    "deploy": "npm run site && gh-pages -d ./dist",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./packages && npm run lint:prettier",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "release": "node ./scripts/release.js  --conventional-prerelease",
    "site": "dumi build",
    "size": "npm run build && size-limit",
    "size:build": "cross-env PRO_COMPONENTS_CI='CI' webpack",
    "start": "dumi dev",
    "sync:tnpm": "node -e 'require(\"./scripts/syncTNPM\")()'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:update": "umi-test --updateSnapshot",
    "tsc": "tsc --noEmit",
    "tsc:duplicate": "tsc -p ./tests/tsconfig.duplicate.json",
    "update:deps": "yarn upgrade"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "resolutions": {
    "@types/responselike": "^1.0.0",

    "prettier": "2.x",
    "typescript": "~4.7.4"
  },
